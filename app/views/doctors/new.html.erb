<h1> Add new Doctor </h1> 
<%= form_with model: @doctor do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :specialization %><br>
    <%= form.text_area :specialization%>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>



